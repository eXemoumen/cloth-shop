{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/custmizer/page.jsx", "src/app/Canvas/shirt.jsx", "src/app/Canvas/page.jsx", "src/app/Canvas/cameraRing.jsx", "src/app/Canvas/BackDrop.jsx", "src/app/components/Tab.jsx", "src/app/components/Colorpiker.jsx", "src/app/components/Filepicker.jsx", "src/app/components/AIpeaker.jsx"],
  "exclude": ["node_modules"]
}
